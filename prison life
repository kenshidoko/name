--[Variables]--
local NumberOfArrests = 0
local ArrestTime = 0

--[Functions]--
local function GetPlayer(String)
        String = String:lower() -- Turns string to lower case

        for Index, Player in pairs(game:GetService("Players"):GetPlayers()) do
                if string.sub(String, 1, #String) == string.sub(Player.Name:lower(), 1, #String) then
                        return(Player)
                end
        end
end

-- UI
PlayerTextBox:GetPropertyChangedSignal("Text"):Connect(function()
        PlayerTextBoxSuggestions.Text = PlayerTextBox.Text
        if GetPlayer(PlayerTextBox.Text) then
                if GetPlayer(PlayerTextBox.Text).Name ~= game.Players.LocalPlayer.Name then
                        PlayerTextBoxSuggestions.Text = GetPlayer(PlayerTextBox.Text).Name:lower()
                end
        end
end)

PlayerTextBox.FocusLost:Connect(function()
        PlayerTextBoxSuggestions.Text = ""
        if GetPlayer(PlayerTextBox.Text) then
                if GetPlayer(PlayerTextBox.Text).Name ~= game.Players.LocalPlayer.Name then
                        PlayerTextBox.Text = GetPlayer(PlayerTextBox.Text).Name
                        PlayerTextBoxSuggestions.Text = ""
                end
        else
                PlayerTextBox.Text = ""
                wait()
                PlayerTextBoxSuggestions.Text = ""
        end
end)

PowerTextBox:GetPropertyChangedSignal("Text"):Connect(function()
        if PowerTextBox.Text ~= "" and tonumber(PowerTextBox.Text) then
                if tonumber(PowerTextBox.Text) > _G.MaxPower then
                        PowerTextBox.Text = tostring(_G.MaxPower)
                end
                if tonumber(PowerTextBox.Text) < 1 then
                        PowerTextBox.Text = "1"
                end
        else
                PowerTextBox.Text = ""
        end
end)

YOffsetTextBox:GetPropertyChangedSignal("Text"):Connect(function()       
        if YOffsetTextBox.Text ~= "" and tonumber(YOffsetTextBox.Text) then
                if tonumber(YOffsetTextBox.Text) > 12 then
                        YOffsetTextBox.Text = tostring(12)
                end
                if tonumber(YOffsetTextBox.Text) < -12 then
                        YOffsetTextBox.Text = "-12"
                end
        else
                if YOffsetTextBox.Text ~= "-" then
                        YOffsetTextBox.Text = ""
                end
        end        
end)


ExitButton.MouseButton1Click:Connect(function()
        ToggleButton.Text = "OFF"
        UltraArrestScreenGui.Enabled = false
end)

-- Arrest
local function GetPower()
        if PowerTextBox.Text ~= "" or tonumber(PowerTextBox.Text) then
                return(tonumber(PowerTextBox.Text))
        else
                return(_G.DefaultPower)
        end
end

local function GetYOffset()
        if YOffsetTextBox.Text ~= "" or tonumber(YOffsetTextBox.Text) then
                return(tonumber(YOffsetTextBox.Text))
        else
                return(_G.DefaultYOffset)
        end
end

local function TeleportTo(Plr, SayOn)
        coroutine.resume(coroutine.create(function()
                if Plr and Plr.Character then
                        local Player = Plr
                        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                                if Player.Character:FindFirstChild("HumanoidRootPart") then
                                        game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = Player.Character:FindFirstChild("HumanoidRootPart").CFrame + Vector3.new(0, GetYOffset(), 0)
                                        if SayOn then
                                                ToggleButton.Text = "ON"
                                        end        
                                end
                        end
                else
                        if SayOn then
                                ToggleButton.Text = "Invalid Character"
                                ToggleButton.TextColor3 = Color3.fromRGB(239, 63, 40)
                                wait(1)
                                ToggleButton.Text = "OFF"
                                ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                        end     
                end
        end))
end

local function Arrest(Plr)-- Ultra Arrest UI
        game:GetService("RunService"):BindToRenderStep("Ultra Spam Arrest", 1, function()
                coroutine.resume(coroutine.create(function()
                        pcall(function()
                                if Plr and Plr.Name and Plr ~= game.Players.LocalPlayer then
                                        local Player = GetPlayer(PlayerTextBox.Text)

                                        if Player and Player.Character and Player.Character:FindFirstChild("Head") then
                                                local PlrHead = {Player.Character:FindFirstChild("Head")}

                                                if Player and PlrHead ~= ({} or nil) then
                                                        -- Arrest
                                                        local Power =  GetPower()
                                                        for i = 1, Power, 1 do
                                                                coroutine.resume(coroutine.create(function()
                                                                        if ToggleButton.Text == "ON" then
                                                                                local LocalHumanoid = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                                                                                local TargetHumanoid = Player.Character:FindFirstChild("HumanoidRootPart")
                                                                                if (TargetHumanoid.Position - LocalHumanoid.Position).magnitude <= 12 then
                                                                                        for i = 1, Power, 1 do
                                                                                                coroutine.resume(coroutine.create(function()
                                                                                                        workspace.Remote.arrest:InvokeServer(unpack(PlrHead))
                                                                                                end))
                                                                                        end
                                                                                        NumberOfArrests = NumberOfArrests + 1
                                                                                else
                                                                                        if ToggleButton.Text == "ON" then
                                                                                                TeleportTo(Player)
                                                                                        end
                                                                                end
                                                                                wait()
                                                                                for i, v in pairs(PlrHead[1]:GetChildren()) do
                                                                                        if v:IsA("BillboardGui") then
                                                                                                v:Destroy()
                                                                                        end
                                                                                end
                                                                        end
                                                                end))
                                                        end
                                                end
                                        end
                                end
                        end)
                end))
        end)
end

local function DestroyHead(Plr)
        coroutine.resume(coroutine.create(function()
                pcall(function()
                        if Plr and Plr.Character and Plr.Character:FindFirstChild("Head")then
                                Plr.Character:FindFirstChild("Head"):ClearAllChildren()
                        end
                end)
        end))
end

