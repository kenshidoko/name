game.Players.LocalPlayer.CharacterAdded:Connect(function() wait(.1)
	if _G.GodModeActive == true then
		Respawn(GodPosition)
                NewHum()
		dieria()

                
	
	end
end)
if _G.GodModeActive then
	Respawn(GodPosition)
	NewHum()
end
