                TeleportTo(GetPlayer(PlayerTextBox.Text), true)
                Arrest(GetPlayer(PlayerTextBox.Text))
                NumberOfArrests = 0
                ArrestTime = tick()
        else
                ToggleButton.Text = "OFF"
                warn("\n\nARREST DATA: \nTarget:"..(GetPlayer(PlayerTextBox.Text) and (GetPlayer(PlayerTextBox.Text).Name) or PlayerTextBox.Text).."\nTotal Arrests: "..tostring(NumberOfArrests).."\nArrests Per Second: "..math.floor(((NumberOfArrests)/(tick() - ArrestTime)) + 0.5).."\nTime: "..tostring(math.floor(((tick() - ArrestTime) * 100) + 0.5) / 100).." seconds\nLevel: "..GetPower())
                game:GetService("RunService"):UnbindFromRenderStep("Ultra Spam Arrest")
        end
end)
