elseif cht == (";godmode") then
 _G.GodModeActive = true

-- Variables
local GodPosition = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
local CameraCFrame = game.Workspace.Camera.CFrame
local PreviousWalkSpeed = 25
local player = game.Players.LocalPlayer

-- Respawn 
local function Respawn(CFrame)
	if _G.GodModeActive == true then
		local hum = player.Character.Humanoid
		local hr = player.Character.HumanoidRootPart

		if hum and hr then
			hr.CFrame = CFrame
		end
	end
end

local function NewHum()
	pcall(function()
		-- Old Humanoid
		local CurrentHum = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
		CurrentHum.Name = "1"

		-- New Humanoid
		local NewHum = CurrentHum:Clone()
		NewHum.Name = "Humanoid"
		NewHum.Parent = CurrentHum.Parent
		NewHum.DisplayDistanceType = "None"
		NewHum.DisplayDistanceType = "None"
		NewHum.WalkSpeed = PreviousWalkSpeed

		-- Adjust camera
		game.Workspace.Camera.CameraType = Enum.CameraType.Follow
		game.Workspace.CurrentCamera.CameraSubject = NewHum

		-- Delete Old Humanoid
		CurrentHum:Destroy()

		-- When the Humanoid Gets Removed, record CFrames
		NewHum.AncestryChanged:Connect(function()
			if not NewHum:IsDescendantOf(game) then
				print("changing char!!")
				CameraCFrame = game.Workspace.Camera.CFrame

				game.Workspace.Camera.CameraType = Enum.CameraType.Fixed

				PreviousWalkSpeed = NewHum.WalkSpeed

				GodPosition = player.Character.HumanoidRootPart.CFrame
			end
		end)

		-- Enable animations (Other players won't see your animations)
		if game.Players.LocalPlayer.Character:FindFirstChild("Animate", true) then
			game.Players.LocalPlayer.Character:FindFirstChild("Animate", true).Disabled = true
			game.Players.LocalPlayer.Character:FindFirstChild("Animate", true).Disabled = false
		end

		-- Delete ForceField
		local tick_ = tick()
		pcall(function()
			repeat wait() until (game.Players.LocalPlayer.Character:FindFirstChild("ForceField", true) or tick() - tick_ > 1)
			if game.Players.LocalPlayer.Character:FindFirstChild("ForceField", true) then
			game.Players.LocalPlayer.Character:FindFirstChild("ForceField", true):Destroy()
			end
		end)
	end)
end

--camera function the the humanoid camera never fkn breaks
local function dieria()
game.Workspace.Camera.CameraType = "Custom" 
game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
end

